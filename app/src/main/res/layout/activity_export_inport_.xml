<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Ui.Activity.ExportInport_Activity">
    <RelativeLayout
        android:id="@+id/topactionbar_expo"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp70"
        android:background="@drawable/action_bar">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"
            android:layout_alignParentBottom="true">

            <ImageView
                android:id="@+id/back_icon_export_import"
                android:layout_width="@dimen/dp20"
                android:layout_height="@dimen/dp20"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:elevation="8dp"
                android:layout_marginStart="@dimen/dp12"
                android:src="@drawable/back_btn"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:textColor="@color/white"
                android:textSize="@dimen/dp16"
                android:fontFamily="@font/poppins_regular"
                android:text="Export/Import Macros"/>
        </RelativeLayout>
    </RelativeLayout>
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home_frag_bg">


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        app:cardBackgroundColor="#70707070"
        app:cardCornerRadius="@dimen/dp18"
        android:layout_marginHorizontal="@dimen/dp26">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp113"
                android:layout_marginTop="@dimen/dp24"
                app:cardCornerRadius="@dimen/dp8"
                android:layout_marginHorizontal="@dimen/dp26">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp45"
                        android:gravity="center"
                        android:textColor="@color/black"
                        android:fontFamily="@font/poppins_regular"
                        android:textSize="@dimen/sp12"
                        android:text="Export"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#57707070"/>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:weightSum="2"
                        android:layout_marginTop="2dp">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:layout_height="match_parent">

                            <ImageView
                                android:layout_width="@dimen/dp32"
                                android:layout_height="@dimen/dp32"
                                android:src="@drawable/storage_icon"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/sp10"
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/dp4"
                                android:fontFamily="@font/poppins_regular"
                                android:text="Storage"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:layout_height="match_parent">

                            <ImageView
                                android:layout_width="@dimen/dp32"
                                android:layout_height="@dimen/dp32"
                                android:src="@drawable/android_share"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/sp10"
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/dp4"
                                android:fontFamily="@font/poppins_regular"
                                android:text="Android share"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp113"
                android:layout_marginTop="@dimen/dp30"
                android:layout_marginBottom="@dimen/dp24"
                app:cardCornerRadius="@dimen/dp8"
                android:layout_marginHorizontal="@dimen/dp24">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp45"
                        android:gravity="center"
                        android:textColor="@color/black"
                        android:fontFamily="@font/poppins_regular"
                        android:textSize="@dimen/sp12"
                        android:text="Import"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#57707070"/>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_marginTop="2dp">

                        <ImageView
                            android:layout_width="@dimen/dp32"
                            android:layout_height="@dimen/dp32"
                            android:src="@drawable/storage_icon"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/sp10"
                            android:textColor="@color/black"
                            android:layout_marginTop="@dimen/dp4"
                            android:fontFamily="@font/poppins_regular"
                            android:text="Storage"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>


        </LinearLayout>
    </androidx.cardview.widget.CardView>
</RelativeLayout>
</LinearLayout>